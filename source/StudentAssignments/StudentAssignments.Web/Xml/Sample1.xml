<?xml version="1.0" encoding="UTF-8"?>
<Шаблон>
  <Документы>
    <Документ Ид="1" Название="Курсовая" Тип="Файл"></Документ>
    <Документ Ид="2" Название="Комментарий проверяющего" Тип="Текст" Обязательный="false"></Документ>
  </Документы>
  <Этапы>
    <Этап Срок="3" Название="Этап 1: загрузка курсовой">
      <ВходныеДокументы>
        <Документ Ид="2"></Документ>
      </ВходныеДокументы>
      <ВыходныеДокументы>
        <Документ Ид="1"></Документ>
      </ВыходныеДокументы>
    </Этап>
    <Этап Срок="1" Название="Этап 2: проверка" Возврат="true">
      <ВходныеДокументы>
        <Документ Ид="1"></Документ>
      </ВходныеДокументы>
      <ВыходныеДокументы>
        <Документ Ид="2"></Документ>
      </ВыходныеДокументы>
    </Этап>
    <Этап Срок="1" Название="Этап 3: оценка">
      <ВходныеДокументы>
        <Документ Ид="1"></Документ>
        <Документ Ид="2"></Документ>
      </ВходныеДокументы>
    </Этап>
  </Этапы>
</Шаблон>